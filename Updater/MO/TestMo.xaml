<Window x:Class="Updater.MO.TestMo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Updater.MO"
        mc:Ignorable="d"
        Title="TestMo" Height="450" Width="400">
    <Grid>
        <StackPanel Grid.Row="1" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Вкл/выкл окно статусов" MaxHeight="28" MinHeight="25" MinWidth="145" Click="TestEnableDisableBuildStatusGrid" Visibility="Visible"/>
            <Label/>
            <Button Content="Заполнить билды" MaxHeight="28" MinHeight="25" MinWidth="145" Click="TestSetBuildResultInUi" Visibility="Visible"/>
            <Label/>
            <Button Content="Очистить список" MaxHeight="28" MinHeight="25" MinWidth="145" Click="TestClearBuildResultInUi" Visibility="Visible"/>
        </StackPanel>
    </Grid>
</Window>
